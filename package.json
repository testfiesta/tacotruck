    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "axios": "^1.10.0",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "delay": "^6.0.0",
    "fast-xml-parser": "^5.2.5",
    "form-data": "^4.0.3",
    "gradient-string": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@types/node": "^24.0.13",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3"
  }
}
