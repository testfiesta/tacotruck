{
  "name": "@testfiesta/tacotruck",
  "type": "module",
  "version": "0.3.0",
  "description": "Test/QA data pipeline by TestFiesta",
  "author": "TestFiesta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testfiesta/tacotruck.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli/index": {
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "bin": {
    "tacotruck": "bin/index.js"
  },
  "files": [
    "bin",
    "configs",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "release": "bumpp --commit --push --tag"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "axios": "^1.10.0",
    "commander": "^14.0.0",
    "delay": "^6.0.0",
    "fast-xml-parser": "^5.2.5",
    "form-data": "^4.0.3",
    "gradient-string": "^3.0.0",
    "ky": "^1.8.2",
    "p-limit": "^6.2.0",
    "p-queue": "^8.1.0",
    "p-throttle": "^7.0.0",
    "std-env": "^3.9.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
