{
  "name": "@testfiesta/tacotruck",
  "type": "module",
  "version": "0.3.0",
  "description": "Test/QA data pipeline by TestFiesta",
  "author": "TestFiesta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testfiesta/tacotruck.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli/index": {
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "bin": {
