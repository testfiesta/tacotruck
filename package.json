{
  "name": "@testfiesta/tacotruck",
  "type": "module",
  "version": "1.0.0-beta.25",
  "description": "Test/QA data pipeline by TestFiesta",
  "author": "TestFiesta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testfiesta/tacotruck.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli/index": {
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "bin": {
    "tacotruck": "bin/index.js"
  },
  "files": [
    "bin",
    "configs",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:binaries": "bun run scripts/build-cli-bundles.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npx simple-git-hooks",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "release": "bumpp --commit --push --tag",
    "tf:pipe": "tsx scripts/pipeline-submit-tf.ts"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@inquirer/prompts": "^7.8.6",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "delay": "^6.0.0",
    "fast-xml-parser": "^5.3.0",
    "form-data": "^4.0.4",
    "gradient-string": "^3.0.0",
    "ofetch": "^1.4.1",
    "p-limit": "^6.2.0",
    "p-queue": "^8.1.1",
    "p-throttle": "^7.0.0",
    "std-env": "^3.9.0",
    "tinyglobby": "^0.2.15",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@types/bun": "^1.3.0",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.3.1",
    "eslint": "^9.37.0",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.4",
    "msw": "^2.11.5",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.7.0",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
