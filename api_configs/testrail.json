{
  "name": "testrail",
  "requests_per_second": 2,
  "base_path": "index.php?",
  "data_key": "{endpoint}",
  "denormalized_keys": {
    "cases": {
      "projects": {
        "suites": "project_id"
       }
    }
  },
  "paging": {
    "location": "response",
    "link_key": "_links.next"
  },
  "auth": {
    "type": "basic"
  },
  "source": {
    "projects": {
      "response_data_key": "projects",
      "target": "projects",
      "endpoints": {
        "index": {
          "path": "api/v2/get_projects",
          "required": []
        },
        "get": {
          "path": "api/v2/get_project/{id}",
          "required": ["id"]
        }
      },
      "mapping": {
        "id": "external_id",
        "name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "suites": {
      "response_data_key": "suites",
      "target": "suites",
      "endpoints": {
        "index": {
          "path": "api/v2/get_suites/{projects_id}",
          "required": ["projects_id"]
        },
        "get": {
          "path": "api/v2/get_suite/{id}",
          "required": ["id"]
        }
      },
      "mapping": {
        "id": "external_id",
        "projects_id": "projects_id",
        "name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "cases": {
      "response_data_key": "cases",
      "target": "cases",
      "endpoints": {
        "index": {
          "path": "api/v2/get_cases/{projects_id}&suite_id={suites_id}",
          "required": ["projects_id", "suites_id"]
        },
        "get": {
          "path": "api/v2/get_case/{id}",
          "required": ["id"]
        }
      },
      "mapping": {
        "created_on": "created_at",
        "id": "external_id",
        "projects_id": "projects_id",
        "suites_id": "suites_id",
        "title": "name",
        "updated_on": "updated_at"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "plans": {
      "response_data_key": "plans",
      "target": "plans",
      "endpoints": {
        "index": {
          "path": "api/v2/get_plans/{projects_id}",
          "required": ["projects_id"]
        },
        "get": {
          "path": "api/v2/get_plan/{id}",
          "required": ["id"]
        }
      },
      "mapping": {
        "created_on": "created_on",
        "id": "external_id",
        "projects_id": "projects_id",
        "name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "runs": {
      "response_data_key": "runs",
      "target": "runs",
      "endpoints": {
        "index": {
          "path": "api/v2/get_runs/{projects_id}",
          "required": ["projects_id"]
        },
        "get": {
          "path": "api/v2/get_run/{id}",
          "required": ["id"]
        }
      },
      "mapping": {
        "created_on": "created_at",
        "id": "external_id",
        "projects_id": "projects_id",
        "name": "name",
        "updated_on": "updated_at"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "tests": {
      "response_data_key": "tests",
      "target": "executions",
      "endpoints": {
        "index": {
          "path": "api/v2/get_tests/{runs_id}",
          "required": ["runs_id"]
        },
        "get": {
          "path": "api/v2/get_test/{id}",
          "required": ["id"]
        }
      },
      "mapping": {
        "id": "external_id",
        "runs_id": "runs_id",
        "title": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    }
  },
  "target": {
    "projects": {
      "target": "projects",
      "endpoints": {
        "create": {
          "path": "api/v2/add_project",
          "required": ["name"]
        },
        "update": {
          "path": "api/v2/update_project/{id}",
          "required": ["id", "name"]
        }
      },
      "mapping": {
        "id": "external_id",
        "name": "name"
      }
    },
    "suites": {
      "target": "suites",
      "endpoints": {
        "create": {
          "path": "api/v2/add_suite/{projects_id}",
          "required": ["name", "projects_id"]
        },
        "update": {
          "path": "api/v2/update_suite/{id}",
          "required": ["id", "name"]
        }
      },
      "mapping": {
