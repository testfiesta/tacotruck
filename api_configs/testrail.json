{
  "name": "testrail",
  "requests_per_second": 2,
  "base_path": "index.php?",
  "data_key": "{endpoint}",
  "denormalized_keys": {
    "cases": {
      "projects": {
        "suites": "project_id"
       }
    }
  },
  "paging": {
    "location": "response",
    "link_key": "_links.next"
  },
  "auth": {
    "type": "basic"
  },
  "source": {
    "projects": {
      "response_data_key": "projects",
      "target": "projects",
      "path": "api/v2/get_projects",
      "mapping": {
	"id": "external_id",
	"name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "suites": {
      "response_data_key": "suites",
      "target": "suites",
      "path": "api/v2/get_suites/{projects.id}",
      "mapping": {
        "id": "external_id",
        "name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "cases": {
      "response_data_key": "cases",
      "target": "cases",
      "path": "api/v2/get_cases/{projects.id}&suite_id={suites.id}",
      "mapping": {
        "created_on": "created_at",
        "id": "external_id",
        "title": "name",
        "updated_on": "updated_at"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "plans": {
      "response_data_key": "plans",
      "target": "plans",
      "path": "api/v2/get_plans/{projects.id}",
      "mapping": {
	"created_on": "created_on",
	"id": "external_id",
	"name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "runs": {
      "response_data_key": "runs",
      "target": "runs",
      "path": "api/v2/get_runs/{projects.id}",
      "mapping": {
	"created_on": "created_at",
	"id": "external_id",
	"name": "name",
	"updated_on": "updated_at"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "tests": {
      "response_data_key": "tests",
      "target": "executions",
      "path": "api/v2/get_tests/{runs.id}",
      "mapping": {
	"id": "external_id",
	"title": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    }
  }
}
