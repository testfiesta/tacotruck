          "required": ["id", "title"]
        }
      },
      "mapping": {
        "id": "external_id",
        "sections_id": "sections_id",
        "title": "title"
      }
    },
    "plans": {
      "target": "plans",
      "endpoints": {
        "create": {
          "path": "api/v2/add_plan/{projects_id}",
          "required": ["name", "projects_id"]
        },
        "update": {
          "path": "api/v2/update_plan/{id}",
          "required": ["id", "name"]
        }
      },
      "mapping": {
        "id": "external_id",
        "projects_id": "projects_id",
        "name": "name"
      }
    },
    "runs": {
      "target": "runs",
      "endpoints": {
        "create": {
          "path": "api/v2/add_run/{projects_id}",
          "required": ["name", "projects_id"]
        },
        "update": {
          "path": "api/v2/update_run/{id}",
          "required": ["id", "name"]
        }
      },
      "mapping": {
        "id": "external_id",
        "projects_id": "projects_id",
        "name": "name"
      }
    }
  }
