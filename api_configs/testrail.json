{
  "name": "testrail",
  "requests_per_second": 2,
  "base_path": "index.php?",
  "data_key": "{endpoint}",
  "denormalized_keys": {
    "cases": {
      "suites": {
        "projects.id": "project_id" 
       }
    }
  },
  "paging": {
    "location": "response",
    "link_key": "_links.next"
  },
  "auth": {
    "type": "basic"
  },
  "source": {
    "projects": {
      "response_data_key": "projects",
      "target": "projects",
      "endpoints": {
        "index": {
          "path": "api/v2/get_projects",
          "required": []
        },
        "get": {
          "path": "api/v2/get_project/{id}"
        }
      },
      "mapping": {
        "id": "source_id",
        "name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "suites": {
      "response_data_key": "suites",
      "target": "suites",
      "endpoints": {
        "index": {
          "path": "api/v2/get_suites/{projects.id}",
          "required": ["projects.id"]
        },
        "get": {
          "path": "api/v2/get_suite/{id}"
        }
      },
      "mapping": {
        "id": "source_id",
        "name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "cases": {
      "response_data_key": "cases",
      "target": "cases",
      "endpoints": {
        "index": {
          "path": "api/v2/get_cases/{projects.id}&suite_id={suites.id}",
          "required": ["projects.id", "suites.id"]
        },
        "get": {
          "path": "api/v2/get_case/{id}"
        }
      },
      "mapping": {
        "created_on": "created_at",
        "id": "source_id",
        "title": "name",
        "updated_on": "updated_at"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "plans": {
      "response_data_key": "plans",
      "target": "plans",
      "endpoints": {
        "index": {
          "path": "api/v2/get_plans/{projects.id}",
          "required": ["projects.id"]
        },
        "get": {
          "path": "api/v2/get_plan/{id}"
        }
      },
      "mapping": {
	"created_on": "created_on",
	"id": "source_id",
	"name": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "runs": {
      "response_data_key": "runs",
      "target": "runs",
      "endpoints": {
        "index": {
          "path": "api/v2/get_runs/{projects.id}",
          "required": ["projects.id"]
        },
        "get": {
          "path": "api/v2/get_run/{id}"
        }
      },
      "mapping": {
	"created_on": "created_at",
	"id": "source_id",
	"name": "name",
	"updated_on": "updated_at"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    },
    "tests": {
      "response_data_key": "tests",
      "target": "executions",
      "endpoints": {
        "index": {
          "path": "api/v2/get_tests/{runs.id}",
          "required": ["runs.id"]
        },
        "get": {
          "path": "api/v2/get_test/{id}"
        }
      },
      "mapping": {
	"id": "source_id",
	"title": "name"
      },
      "limit": {
        "type": "count",
        "value": "1000"
      }
    }
  },
  "target": {
    "projects": {
      "target": "projects",
      "endpoints": {
        "create": {
          "single_path": "api/v2/add_project",
          "required": ["name"]
        },
        "update": {
          "path": "api/v2/update_project/{id}",
          "required": ["id", "name"],
          "update_key": "id"
        }
      }
    },
    "suites": {
      "target": "suites",
      "endpoints": {
        "create": {
          "single_path": "api/v2/add_suite/{projects.id}",
          "required": ["name", "projects.id"]
        },
        "update": {
          "path": "api/v2/update_suite/{id}",
          "required": ["id", "name"],
          "update_key": "id"
        }
      }
    },
    "cases": {
      "target": "cases",
      "endpoints": {
        "create": {
          "single_path": "api/v2/add_case/{sections.id}",
          "required": ["title", "sections.id"]
        },
        "update": {
          "path": "api/v2/update_case/{id}",
          "required": ["id", "title"],
          "update_key": "id"
        }
      }
    },
    "plans": {
      "target": "plans",
      "endpoints": {
        "create": {
          "single_path": "api/v2/add_plan/{projects.id}",
          "required": ["name", "projects.id"]
        },
        "update": {
          "path": "api/v2/update_plan/{id}",
          "required": ["id", "name"],
          "update_key": "id"
        }
      }
    },
    "runs": {
      "target": "runs",
      "endpoints": {
        "create": {
          "single_path": "api/v2/add_run/{projects.id}",
          "required": ["name", "projects.id"]
        },
        "update": {
          "path": "api/v2/update_run/{id}",
          "required": ["id", "name"],
          "update_key": "id"
        }
      }
    }
  }
}
